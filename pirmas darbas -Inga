kintamasis = 1
kintamasis2 = 2

def funkcija():
    print(kintamasis)
    print(kintamasis2)
    print(f'test:{kintamasis2}{kintamasis}')
    print('test: {}{}'.format{kintamasis2, kintamasis})


    #sudetis
def add(x,y):
    return x + y

#atimtis
def subtract(x, y):
    return x - y 

#daugyba
def multiply(x, y):
    return x * y 

#dalyba
def divide(x, y):
    return x / y


